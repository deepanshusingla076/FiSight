# Firebase Configuration - Get from Firebase Console > Project Settings
NEXT_PUBLIC_FIREBASE_API_KEY=your_firebase_api_key_here
NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN=your_project_id.firebaseapp.com
NEXT_PUBLIC_FIREBASE_PROJECT_ID=your_project_id
NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET=your_project_id.firebasestorage.app
NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=123456789012
NEXT_PUBLIC_FIREBASE_APP_ID=1:123456789012:web:abcdef123456789012
NEXT_PUBLIC_FIREBASE_MEASUREMENT_ID=G-ABCDEF1234

# AI Configuration - Get Gemini API key from https://aistudio.google.com/
GEMINI_API_KEY=your_gemini_api_key_here
GOOGLE_GENAI_API_KEY=your_gemini_api_key_here
GOOGLE_AI_API_KEY=your_gemini_api_key_here

# ML API Configuration
NEXT_PUBLIC_ML_API_URL=http://localhost:8000

# Email Configuration - For Gmail: Enable 2FA and generate App Password
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your_email@gmail.com
SMTP_PASS=your_gmail_app_password_here
SMTP_FROM=your_email@gmail.com
SMTP_TO=your_contact_email@domain.com

# Authentication Configuration
NEXTAUTH_URL=http://localhost:9002
NEXTAUTH_SECRET=your_super_secret_key_minimum_32_characters_long

# Feature Flags
FISIGHT_ENABLE_PRETRAINED_MODEL=false
FISIGHT_ENABLE_MCP_SERVER=false

# App Environment
NODE_ENV=development
NEXT_PUBLIC_APP_ENV=development
NEXT_PUBLIC_APP_VERSION=1.0.0

# API Endpoints
NEXT_PUBLIC_API_BASE_URL=http://localhost:9002/api
NEXT_PUBLIC_WEBAPP_URL=http://localhost:9002

# Debug and Analytics
NEXT_PUBLIC_ENABLE_DEBUG_MODE=true
NEXT_PUBLIC_ENABLE_ANALYTICS=false